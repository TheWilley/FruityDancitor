{
  "name": "fruitydancitor",
  "author": "TheWilley",
  "private": true,
  "version": "3.6.0",
  "type": "module",
  "scripts": {
    "start": "serve dist",
    "dev": "vite",
    "devspose": "vite --host",
    "build": "tsc && vite build",
    "build-docker": "tsc && vite build --base=/",
    "deploy": "npm run build && gh-pages -d dist",
    "generate": "node scripts/generateKeybindings.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier src --write",
    "lintfix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "postversion": "git push && git push --tags && npm run deploy && echo 'Please create a new release at https://github.com/TheWilley/FruityDancitor/releases/new'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^2.1.0",
    "array-move": "^4.0.0",
    "file-saver": "^2.0.5",
    "image-to-base64": "^2.2.0",
    "jszip": "^3.10.1",
    "omggif": "^1.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hotkeys-hook": "^4.4.3",
    "react-movable": "^3.0.4",
    "react-redux": "^9.1.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.6",
    "@types/omggif": "^1.0.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.15",
    "daisyui": "^4.7.2",
    "eslint": "^8.54.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "gh-pages": "^6.1.1",
    "jsonfile": "^6.1.0",
    "postcss": "^8.4.28",
    "prettier": "3.1.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.5"
  }
}
